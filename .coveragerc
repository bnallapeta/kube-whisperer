[run]
source = src
branch = True
omit = 
    */tests/*
    */venv/*
    */__init__.py
    */setup.py
    */conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
    raise NotImplementedError
    if TYPE_CHECKING:
    @abstractmethod
    @property
    @classmethod
    @staticmethod
    def __init__
    raise NotImplementedError
    if __name__ == '__main__':
    pass

[html]
directory = coverage_html
title = Coverage Report

[paths]
source =
    src/
    */src/ 